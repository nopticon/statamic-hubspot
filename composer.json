{
    "name": "lwekuiper/statamic-hubspot",
    "license": "proprietary",
    "type": "statamic-addon",
    "require": {
        "php": "^8.2",
        "laravel/framework": "^10.0 || ^11.0 || ^12.0",
        "statamic/cms": "^4.0 || ^5.0 || ^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0 || ^9.0 || ^10.0",
        "laravel/pint": "^1.0"
    },
    "description": "Subscribe forms to HubSpot",
    "authors": [
        {
            "name": "Laurens Kuiper",
            "email": "l@urens.me"
        }
    ],
    "autoload": {
        "psr-4": {
            "Lwekuiper\\StatamicHubspot\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lwekuiper\\StatamicHubspot\\Tests\\": "tests"
        }
    },
    "extra": {
        "statamic": {
            "name": "Statamic HubSpot",
            "description": "HubSpot integration for Statamic",
            "editions": ["lite", "pro"]
        },
        "laravel": {
            "providers": [
                "Lwekuiper\\StatamicHubspot\\ServiceProvider"
            ]
        }
    },
    "scripts": {
        "pint": "pint",
        "pint-test": "pint --test"
    },
    "config": {
        "allow-plugins": {
            "pixelfear/composer-dist-plugin": true
        }
    }
}
